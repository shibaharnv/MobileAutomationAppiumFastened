<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MyTestSuite">
    <listeners>
        <listener class-name="nl.fastned.TestUtils.Listeners"/>
    </listeners>

    <test name="MyTest">
        <groups>
            <run>
                <include name="regression" />
